    <%= simple_form_for(@jobrequest, html: { class: 'form-inline' }) do |f| %>
      <%= f.error_notification %>
      <%= f.input :position, collection: JobRequest::POSITIONS, label: false %>
      <%# f.input :location, collection: JobRequest::LOCATIONS %>
      <%= f.input :start_time, as: :date, label: false %>
      <%= f.button :submit, class: "btn btn-primary" %>
    <% end %>