    <%= simple_form_for(@jobrequest, html: { class: 'form-inline input-daterange input-group' }) do |f| %>
      <%= f.error_notification %>
      <%= f.input :position, collection: JobRequest::POSITIONS, prompt: "What do you need?", label: false %>
      <%= f.input :location, collection: JobRequest::LOCATIONS, prompt: "Select your location", label: false %>
      <%= f.input :start_time, as: :string, label: false, placeholder: 'start date', input_html: {class: 'search_date  form-control'}  %>
      <%# f.input :start_time, as: :string, label: false, placeholder: 'start date', input_html: { data: {datepicker: :datepicker}} %>
      <%# f.input :start_time, as: :datepicker, required: false, :placeholder => 'Select date', input_html: {class: "form-control"}, label: "Expiration date: " %>
      <%= f.button :submit, 'Find', class: "btn btn-primary", id: "find_button" %>
    <% end %>

